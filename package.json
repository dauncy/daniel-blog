{
  "name": "daniel-blog",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "packageManager": "yarn@4.5.0",
  "scripts": {
    "clean": "rm -rf dist",
    "build:css": "npx tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "build:ts": "esbuild src/script.ts --bundle --outfile=dist/js/script.js",
    "build:11ty": "eleventy",
    "build": "yarn clean && yarn build:css && yarn build:11ty",
    "dev:tailwind": "npx tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "dev:11ty": "eleventy --watch --serve",
    "start": "run-p dev:tailwind dev:11ty"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prismjs": "^1.30.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.3"
  }
}
